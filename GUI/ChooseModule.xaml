<UserControl x:Class="EKG_Project.GUI.ChooseModule"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EKG_Project.GUI"
             mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <ComboBox Grid.Row="0" Grid.Column="0" x:Name="comboChooseModule" Margin="0,0,0,0" VerticalAlignment="Top" SelectionChanged="comboChooseModule_SelectionChanged" HorizontalAlignment="Left" Width="121">
            <ComboBoxItem>ECG_Baseline</ComboBoxItem>
            <ComboBoxItem>R_Peaks</ComboBoxItem>
            <ComboBoxItem>HRT</ComboBoxItem>
            <ComboBoxItem>Flutter</ComboBoxItem>
            <ComboBoxItem>HeartAxis</ComboBoxItem>
            <ComboBoxItem>HRV1</ComboBoxItem>
            <ComboBoxItem>Waves</ComboBoxItem>
            <ComboBoxItem>HeartClass</ComboBoxItem>
            <ComboBoxItem>HRV2</ComboBoxItem>
        </ComboBox>
        <StackPanel Name="stackParameters" Grid.Row="1" />
    </Grid>
</UserControl>
